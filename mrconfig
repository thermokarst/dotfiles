[src/qiime2/qiime2]
checkout = git clone https://github.com/qiime2/qiime2 qiime2 && \
    cd qiime2 && \
    git remote rename origin qiime2 && \
    git remote add thermokarst https://github.com/thermokarst/qiime2 && \
    git fetch thermokarst

