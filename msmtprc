account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from matthewrdillon@gmail.com
user matthewrdillon@gmail.com
passwordeval "python $HOME/.mutt/offlineimap.py matthewrdillon@gmail.com imap.gmail.com"
tls on
tls_trust_file /opt/local/etc/openssl/cert.pem

